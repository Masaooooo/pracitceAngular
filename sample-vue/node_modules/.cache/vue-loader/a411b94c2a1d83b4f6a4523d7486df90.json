g\n  },\n  computed: {\n    ...mapState([\n      'products'\n    ])\n  },\n  methods: {\n    ...mapActions([\n      'fetchProducts'\n    ])\n  },\n  created() {\n    this.fetchProducts()\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n  .productList{\n    width: 1370px;\n    margin: auto;\n    display: flex;\n    flex-wrap: wrap;\n    justify-content: space-between;\n  }\n  .productItem{\n    width: 395px;\n    height: 415px;\n    margin-bottom: 38px;\n    padding: 40px 75px 35px;\n    box-sizing: border-box;\n    box-shadow: 0 3px 6px rgba(0, 0, 0, 0.16);\n  }\n  .productTitle{\n    font-size: 12px;\n    font-weight: 700;\n    overflow: hidden;\n    white-space: nowrap;\n    text-overflow: ellipsis;\n    margin-top: 40px;\n  }\n  .productThumb{\n    object-fit: cover;\n    width: 247px;\n    height: 247px;\n  }\n  .tagList{\n    margin-top: 20px;\n    display: flex;\n  }\n  .tagItem{\n    &:not(:first-child){\n      margin-left: 5px;\n    }\n  }\n</style>\n"]}]}